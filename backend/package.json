{
  "name": "pdf-streaming-backend",
  "version": "1.0.0",
  "description": "PDF streaming backend with HTTP range support",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "fastify": "^5.1.0",
    "pdfjs-dist": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.8.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "supertest": "^7.1.4",
    "@types/supertest": "^6.0.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  },
  "keywords": ["pdf", "streaming", "backend", "api"],
  "author": "",
  "license": "MIT"
}
